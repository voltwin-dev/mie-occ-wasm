{
    "name": "mie-occ-wasm",
    "version": "0.0.1",
    "scripts": {
        "setup": "ts-node setup.ts",
        "build:release:single": "cmake --preset release-single && cmake --build --preset release-single",
        "build:debug:single": "cmake --preset debug-single && cmake --build --preset debug-single",
        "build:release:multi": "cmake --preset release-multi && cmake --build --preset release-multi",
        "build:debug:multi": "cmake --preset debug-multi && cmake --build --preset debug-multi",
        "build": "npm run build:release:single && npm run build:debug:single && npm run build:release:multi && npm run build:debug:multi"
    },
    "license": "LGPL-2.1-only",
    "devDependencies": {
        "@types/node": "^24.5.2",
        "ts-node": "^10.9.2"
    }
}
